{
  "name": "supernova",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:tunnel": "ngrok http 3000",
    "export": "next export",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "build-prod": "run-s clean build export",
    "clean": "rimraf .next out",
    "lint": "next lint",
    "check-types": "tsc --noEmit --pretty",
    "test": "jest",
    "commit": "cz",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test dev http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 cypress:headless",
    "prepare": "husky install",
    "git:getReleasesAndTags": "git tag --points-at HEAD | tr '\\r\\n' ' '",
    "git:getCommitSHA": "git rev-parse HEAD",
    "git:getCommitSHA:short": "git rev-parse --short HEAD",
    "git:getCommitRef": "git symbolic-ref HEAD"
  },
  "dependencies": {
    "@sentry/nextjs": "^7.17.4",
    "@tanstack/react-query": "^4.14.5",
    "@tanstack/react-query-devtools": "^4.14.5",
    "ahooks": "^3.7.2",
    "antd": "^4.24.1",
    "axios": "^1.1.3",
    "clsx": "^1.2.1",
    "firebase": "^9.13.0",
    "i18next": "^22.0.4",
    "i18next-http-backend": "^2.0.1",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "mixpanel-browser": "^2.45.0",
    "next": "^12.2.5",
    "next-i18next": "^12.1.0",
    "next-seo": "^5.8.0",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.0.0",
    "sass": "^1.56.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@commitlint/cz-commitlint": "^17.1.2",
    "@next/bundle-analyzer": "^13.0.0",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.0",
    "@types/js-cookie": "^3.0.2",
    "@types/mixpanel-browser": "^2.38.0",
    "@types/node": "^18.11.5",
    "@types/react": "^18.0.23",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "autoprefixer": "^10.4.12",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.13",
    "cypress": "^10.11.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
